{
    "name": "alyasi/dashboard-ui",
    "description": "مكتبة واجهات تحكم جاهزة للاستخدام في مشاريع Laravel - Dashboard UI Library for Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "dashboard",
        "ui",
        "admin",
        "panel",
        "arabic"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Salim Alyasi",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Alyasi\\DashboardUI\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alyasi\\DashboardUI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alyasi\\DashboardUI\\DashboardUIServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
